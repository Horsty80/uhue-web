    "@11ty/eleventy": "^1.0.1",
    "markdown-it": "^13.0.1",
    "node-fetch": "2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.7",
    "@11ty/eleventy": "2.0.0-beta.3",
    "@11ty/eleventy-plugin-vite": "^4.0.0"
